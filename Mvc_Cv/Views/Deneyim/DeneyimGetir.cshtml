@using Mvc_Cv
@model  Mvc_Cv.Models.Entity.TBLDENEYIMLERIM
@{
    ViewBag.Title = "DeneyimGetir";
    Layout = "~/Views/Shared/_AdminLoyout.cshtml";
}

<br />
<h4>Yeni Deneyim Güncelleme Sayfası</h4>
<br />
<form class="form-group" method="post">
    <label>ID</label>
    @Html.TextBoxFor(x => x.ID, new { @class = "form-control" })
    <br />

    <label>Başlık</label>
    @Html.TextBoxFor(x => x.BASLIK, new { @class = "form-control" })
    <br />

    <label>ALT BAŞLIK</label>
    @Html.TextBoxFor(x => x.ALTBASLIK, new { @class = "form-control" })
    <br />

    <label>AÇIKLAMA</label>
    @Html.TextBoxFor(x => x.ACIKLAMA, new { @class = "form-control" })
    <br />

    <label>TARİH</label>
    @Html.TextBoxFor(x => x.TARIH, new { @class = "form-control" })
    <br />
    <button class="btn btn-info">Güncelle</button>

</form>


